{
  "name": "express-api-bootstrap",
  "version": "3.0.0",
  "description": "express-api-bootstrap makes it easy to create stand-alone, production-grade express based API servers that you can \"just run\"",
  "main": "./bin/index.js",
  "bin": {
    "boot": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "test": "jest",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "build": "rollup -c",
    "prepublish": "rm -rf ./bin && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/express-api-bootstrap.git"
  },
  "keywords": [
    "express",
    "api",
    "bootstrap"
  ],
  "files": [
    "bin",
    "types"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/express-api-bootstrap/issues"
  },
  "homepage": "https://github.com/leftstick/express-api-bootstrap#readme",
  "dependencies": {
    "@babel/register": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "babel-preset-umi": "^1.8.1",
    "chalk": "^3.0.0",
    "commander": "^4.0.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7",
    "coveralls": "^3.0.7",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "rollup": "^1.26.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "tsconfig-paths": "^3.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.2"
  }
}
