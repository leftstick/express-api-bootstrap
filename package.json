{
  "name": "express-api-bootstrap",
  "version": "3.0.0",
  "description": "express-api-bootstrap makes it easy to create stand-alone, production-grade express based API servers that you can \"just run\"",
  "main": "./libs/index.js",
  "typings": "./libs/index.d.ts",
  "bin": {
    "boot": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "test": "jest",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "build": "node build",
    "prepublish": "yarn build",
    "postinstall": "node build/generator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/express-api-bootstrap.git"
  },
  "keywords": [
    "express",
    "api",
    "bootstrap"
  ],
  "files": [
    "bin",
    "build",
    "types",
    "libs",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/express-api-bootstrap/issues"
  },
  "homepage": "https://github.com/leftstick/express-api-bootstrap#readme",
  "dependencies": {
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.7.0",
    "@zerollup/ts-transform-paths": "^1.7.6",
    "babel-preset-umi": "^1.8.1",
    "chokidar": "^3.3.0",
    "commander": "^4.0.1",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "lodash.template": "^4.5.0",
    "ora": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "signale": "^1.4.0",
    "tslib": "^1.10.0",
    "typedi": "^0.8.0",
    "typescript": "^3.7.2",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@rollup/plugin-json": "^4.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/signale": "^1.2.1",
    "@types/update-notifier": "^2.5.0",
    "coveralls": "^3.0.7",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "rollup": "^1.27.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^24.1.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^6.2.0"
  }
}
